<?php

namespace {{ projectName }}\Domain\Specification\Infrastructure\User;


use Sfynx\SpecificationBundle\Specification\AbstractSpecification;

class SpecIsRoleAnonymous extends AbstractSpecification
{
    public function isSatisfiedBy(\stdClass $object)
    {
        if ($object->value->isGranted('IS_AUTHENTICATED_ANONYMOUSLY')) {
            return true;
        }

        return false;
    }
}
