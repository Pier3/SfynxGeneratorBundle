    /**
     * @return Response
     */
    public function {{ action }}Action()
    {
        $adapter = new SearchByQueryAdapter();
        $query = $adapter->createQueryFromRequest(new SearchByRequest($this->request, $this->resolver));

        $entities = $this->searchByQueryHandler->process($query);

        return $this->responseHandler->create($entities, Response::HTTP_OK)->getResponse();
    }
