services:
    project.{{ projectName|lower }}.controller.query.{{ entityName|lower }}:
        scope: prototype
        class: {{ projectName|title }}Context\Presentation\Coordination\{{ entityName|title }}\Query\Controller
        arguments:
            - @project.request.strategy
            - @project.resolver.strategy
            - @project.dddcontext.response.handler
            - @project.{{ projectName|lower }}.{{ entityName|lower }}.query.all.handler
            - @project.{{ projectName|lower }}.{{ entityName|lower }}.query.getbyids.handler
            - @project.{{ projectName|lower }}.{{ entityName|lower }}.query.one.handler
